@model IEnumerable<Spark.DataModel.Models.CarServiceType>
@{
    ViewData["Title"] = "GetServiceType";
}

<style>
    body {
        background-color: black;
        background-image: url("data:image/jpeg;base64,");
        background-size: 1500px;
        background-repeat: no-repeat;
    }
</style>


<br />
<br />
<div class="jumbotron backgroundWhite">

    <div class="row">
        <div class="col-6">
            <h2 style="font-family:'Times New Roman'"><i class="fas fa-wrench"></i> &nbsp;Service Type List</h2>

        </div>
        <div class="col-6 text-right">
            <a asp-action="Create" asp-controller="ServiceTypes" class="fas fa-plus btn btn-info">Add Values</a>

        </div>
    </div>

    <br />
    <div>

        <div>
            <table class="table table-striped border">
                <tr class="table-secondary">

                    <th>Service Name</th>
                    <th>Service Price</th>

                    <th></th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>

                        <td>@item.Name</td>
                        <td>@item.Price</td>

                        <td style="width:150px;">
                            <div class="btn-group" role="group">
                                <a class="btn btn-primary text-white" asp-action="EditServiceType" asp-controller="ServiceTypes" asp-route-Id="@item.Id">
                                    <i class="fa fa-edit"></i>
                                </a>
                                <a class="btn btn-success text-white" asp-action="GEtServiceTypeById" asp-controller="ServiceTypes" asp-route-Id="@item.Id">
                                    <i class="far fa-list-alt"></i>
                                </a>
                                <a class="btn btn-danger text-white" asp-action="DeleteServiceType" asp-route-Id="@item.Id">
                                    <i class="fa fa-trash"></i>
                                </a>
                            </div>

                        </td>
                    </tr>
}
            </table>
        </div>
    </div>
</div>